          m[i] =   y = add(m[(x >>>  2) & 0xff], add(acc, brs));
          r[i] = brs = add(m[(y >>> 10) & 0xff], x);
        }
      }
    }
   
    /* public: return a random number between */
    function rand() {
      if(!gnt--) {
        prng(); gnt = 255;
      }
      return r[gnt];
    }
   
    /* public: return internals in an object*/
    function internals(){
      return {a: acc, b: brs, c: cnt, m: m, r: r};
    }
   
    /* return class object */
    return {
      'reset': reset,
      'seed':  seed,
      'prng':  prng,
      'rand':  rand,
      'internals': internals
    };

        onSecondLevelMenuClicked(event) {
            this.$firstLevelMenu.removeClass(defaults.classes.open);
            this.$collapseMenu.removeClass(defaults.classes.open);
            this.$firstLevelMenu.removeClass(defaults.classes.subMenuOpen);

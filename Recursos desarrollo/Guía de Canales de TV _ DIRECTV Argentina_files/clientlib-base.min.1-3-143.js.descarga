    for (let i = 0; i < data.length; i++) {
        let newdate = this.offerLeftDays(new Date(`${data[i].expiration_date}`), data[i].expiration_date);
        if (newdate < acc) {
            acc = newdate;
            optionSele = [];
            optionSele.push(data[i]);
        }
    }
    return optionSele;
};


            const hasUTMSource = parameterUrl !== null && parameterUrl !== "";

            if (hasUTMSource) {
                this.helper.setCookie(defaults.values.UTMSource, parameterUrl, 1800);
                this.helper.setCookie(defaults.values.LPUTMSource, parameterUrl, 1800);

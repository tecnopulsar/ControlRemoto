            const $this = this;
            this.button = $(this.element).find('button');
            this.button.on('click', function() {
                $this.checkBeforeSending();
            });
        }

        checkBeforeSending() {
            let cookies = document.cookie.split(';')
            let type = "";
            let logged = false;
            cookies.forEach(x => {
                if (x.indexOf('SSOType') > -1) {
                    type = x.split('=')[1];
                }

                if (x.indexOf('SSONumber') > -1) {
                    logged = true;
                }
            });

            this.checkAnyUser(type, logged);

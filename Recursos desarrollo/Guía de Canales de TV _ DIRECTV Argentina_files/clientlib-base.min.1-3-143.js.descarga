         */
        updateHtmlMenuItems(text) {
            this.$myDirectvMenu.children().remove();
            let $result = $(text);
            this.addLinksEvent($result.find(`${defaults.selectors.myDirectvFirstLevelMenu} li, ${defaults.selectors.myDirectvMenuItemsAuth}`));

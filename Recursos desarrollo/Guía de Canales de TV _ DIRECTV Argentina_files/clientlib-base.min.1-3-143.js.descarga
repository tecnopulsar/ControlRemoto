        allow = (e.key == "." || e.key == "," || e.key == "Delete" || e.key == "Backspace");
    } else {
        allow = (e.key == "Delete" || e.key == "Backspace");
    }

    if (!res) {
        if (allow) {
            return true;
        } else {
            this.stopPropagation(e);
        }
    }
};

